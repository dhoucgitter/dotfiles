[user]
	name = David Houck
	email = David.Houck@sas.com
[core]
	eol = lf
	autocrlf = input
[merge]
	renormalize = true
[push]
	default = upstream
[alias]
	a = add
	ap = add --patch
	b = branch -vv
	ba = branch -vv --all
	ci = commit
	amend = commit --amend
	co = checkout
	cp = cherry-pick --no-commit
	d = difftool --no-prompt
	r = remote
	m = merge --squash
	rmt = remote -v
	suno = status -suno
	s-b = status -s -b
	st = status
	br = branch
	last = log -1
[log]
	decorate = true
