[user]
	name = David Houck
	email = David.Houck@sas.com
[core]
	eol = lf
	autocrlf = input
	abbrev = 7
[merge]
	renormalize = true
[push]
	default = upstream
[alias]
	a = add
	ap = add --patch
	b = branch -vv			; very verbose branch listing
	ba = branch -vv --all		; ... with remote branches included
	ci = commit
	cid = commit --dry-run
	cli = clean -i
	amend = commit --amend
	co = checkout
	cp = cherry-pick --no-commit
	d = difftool --no-prompt
	dc = diff --check
	ls = ls-files
	r = remote			; remote operations
	m = merge --squash		; merge all commits into current working tree;
	rmt = remote -v
	suno = status -suno
	s-b = status -s -b		; terse status
	st = status -uno
	br = branch
	last = log -1 --abbrev-commit --stat
	logo = log --abbrev-commit --oneline
	brm = branch --merged
	stu = status -u
	rls = review --list
	unstage = reset HEAD --
	unstageall = reset HEAD
	diffc = diff --cached
	hide = update-index --assume-unchanged
	unhide = update-index --no-assume-unchanged
[log]
	decorate = true
[github]
	user = dhoucgitter
[color]
	ui = true
	ui = auto
